echo "Running custom boot script..."
setenv custom_uboot "/u-boot-verdin-imx8mp.bin"
setenv loadaddr 0x40200000
fatload mmc 1:1 ${loadaddr} ${custom_uboot}
echo "Jumping to custom U-Boot at ${loadaddr}"
go ${loadaddr}
